<main>
    <h1>Tarjeta de producto</h1>
    {{#with productById}}
        <div id="productCard">
            <img src="" alt="{{thumbnails}}">
            <div id="productInfo">
                <h2>Informacion del producto</h2>
                <ul>
                    <li>Nombre Producto: {{title}}</li>
                    <li>Code:{{code}}</li>
                    <li>Precio:${{price}}</li>
                </ul>
            </div>
        </div>
        <div id="productDescription">
            <h2>Description</h2>
            <p>{{description}}</p>
            
        </div>
    {{/with}}
</main>
<script src="/js/index.js"></script>